alias dc='docker-compose'
alias dcd='docker-compose down --remove-orphans'
alias dcb='docker-compose build'
alias dcu='docker-compose up -d --force-recreate'
alias ls='ls -lah --color'
alias cls='clear && echo -en "\e[3J"'
alias cat='batcat'
